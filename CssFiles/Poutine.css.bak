/*Making the heading pop out more;making it more unique*/
h1{
	text-align:center;
	font-size:60px;
	font-family:Lucida Handwriting;
	color:#793009;
}

/*Change in font to see what is a heading and what is informational text*/
body{
	font-family:Lucida Console;
	background-color:#C77B19;
	color:#793009;
}

/*Added indents to make text look more like paragraphs*/
p{
	margin:auto;
	text-indent:3%;
}

/*Backwards key shouldn't pop out so much so text is smaller than other text*/
.backwards{
	font-size:14px;
	margin:80px;
}

.ListSpaces{
	margin-bottom:7px;
}

.HistoryListSpaces{
	margin-bottom:12px;
}

/*To show difference between sub heading and text*/
h2{
	 text-decoration-line: underline;
}

#Center{
  display: block;
  margin-left: auto;
  margin-right: auto;
  
	border-color :black;
	border-width : medium;
	border-style : solid;
}
.References{
	font-size:11px;
	text-indent:0%;
	margin-bottom:10px;
}
/*To prevent too much text to touch each other (too text heavy)*/
h3{
	margin-bottom:1px;
}

#MarginTop{
	margin-top:30px;
}

.MarginBottom{
	margin-bottom:10px;
}















/* Add a soft brown background color to the top navigation */
.topnav {
  background-color:#7C3C0A;
  overflow: hidden;
}

/* Style the links inside the navigation bar */
.topnav a {
  float: left;
  color: white;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size:17px;
}

/* Change the color of links on hover */
.topnav a:hover {
  background-color:#A45C26;
  color: white;
}

/* Add a dark brown to the active/current link */
.topnav a.active {
  background-color:#511F02;
  color:white;
}